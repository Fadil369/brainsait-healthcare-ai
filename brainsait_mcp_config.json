{
  "mcpServers": {
    "brainsait-masterlinc": {
      "command": "python",
      "args": ["-m", "mcp_servers.brainsait_master"],
      "env": {
        "BRAINSAIT_API_KEY": "${BRAINSAIT_API_KEY}",
        "NPHIES_ENDPOINT": "${NPHIES_ENDPOINT}",
        "FHIR_BASE_URL": "${FHIR_BASE_URL}",
        "HIPAA_AUDIT_LOG": "${HIPAA_AUDIT_LOG}"
      }
    },
    "healthcare-workflows": {
      "command": "python", 
      "args": ["-m", "mcp_servers.healthcare_workflows"],
      "env": {
        "N8N_API_KEY": "${N8N_API_KEY}",
        "N8N_WEBHOOK_URL": "${N8N_WEBHOOK_URL}",
        "CLAUDE_API_KEY": "${CLAUDE_API_KEY}"
      }
    },
    "nphies-compliance": {
      "command": "python",
      "args": ["-m", "mcp_servers.nphies_compliance"],
      "env": {
        "NPHIES_CLIENT_ID": "${NPHIES_CLIENT_ID}",
        "NPHIES_CLIENT_SECRET": "${NPHIES_CLIENT_SECRET}",
        "SAUDI_MOH_ENDPOINT": "${SAUDI_MOH_ENDPOINT}"
      }
    },
    "cloudflare-worker": {
      "command": "node",
      "args": ["mcp-servers/cloudflare-worker.js"],
      "env": {
        "CLOUDFLARE_API_TOKEN": "${CLOUDFLARE_API_TOKEN}",
        "CLOUDFLARE_ACCOUNT_ID": "${CLOUDFLARE_ACCOUNT_ID}"
      }
    },
    "n8n-workflows": {
      "command": "python",
      "args": ["-m", "n8n"],
      "env": {
        "N8N_BASIC_AUTH_ACTIVE": "true",
        "N8N_BASIC_AUTH_USER": "${N8N_USER}",
        "N8N_BASIC_AUTH_PASSWORD": "${N8N_PASSWORD}",
        "WEBHOOK_URL": "${N8N_WEBHOOK_URL}"
      }
    }
  }
}